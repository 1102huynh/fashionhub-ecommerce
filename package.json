{
  "name": "astro-fashion-hub",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "kill:4321": "powershell -Command \"Get-NetTCPConnection -LocalPort 4321 -ErrorAction SilentlyContinue | ForEach-Object { Stop-Process -Id $_.OwningProcess -Force -ErrorAction SilentlyContinue }\"",
    "dev": "npm run kill:4321 && astro dev --port 4321",
    "dev:frontend": "npm run kill:4321 && astro dev --port 4321",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "astro build",
    "build:all": "astro build && cd backend && npm run build",
    "preview": "npm run kill:4321 && astro preview --port 4321",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/node": "^9.5.1",
    "astro": "^5.16.4",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
